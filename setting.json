{
  "name-web":"STACKOVERFLOW MASKODING",
  "author":"MASKODING",
  "remove-comment-html":true,
  "target":"https://www.prabhatkhabar.com",
  "name-folder-sitemap":"sitemap",
  "element-remove":[
    "noscript",
    "meta",
    "iframe",
    "embed",
    "ins",
    "form",
    ".js-consent-banner",
    ".js-dismissable-hero",
    ".js-sidebar-zone",
    "#teams-promo",
    ".quickedit"
  ],
  "costom-element-remove":[
    {
      "target":"https://www.prabhatkhabar.com",
      "element-remove-selector":[
        ".js-dismissable-hero",
        ".lazy-loading"
        ".adBox"
        ".belowBreakingNews"
       
              
      ],
      "replace-string":[
        {
          "target":"https://www.prabhatkhabar.com",
          "replace":"${nameWeb}"
        }
      ]
    },
    {
      "target":"https://www.prabhatkhabar.com",
      "element-remove-selector":[
        ".baca",
        ".op-interactive",
        "#iklan_anchor",
        ".box__reserved.noreserved.mt20.mb20",
        ".box__reserved.mh10.nativeads",
        ".box__reserved.mh27.sticky",
        ".box__reserved.mh60.sticky",
        ".box__reserved.ivideo_box",
        ".box__reserved.fb_box.ac.mb20.bgwhite.bdr.facebook",
        ".box__reserved.mh10"
      ],
      "replace-string":[]
    },
    {
      "target":"www.itb.ac.id",
      "element-remove-selector":[
        ".box-header",
        ".box-language",
        ".section-10",
        ".sidebar.active"
      ],
      "replace-string":[]
    },
    {
      "target":"repositori.usu.ac.id",
      "element-remove-selector":[
        "header",
        ".trail-wrapper",
        "footer"
      ],
      "replace-string":[]
    }
  ],
  "inject-element-head":[
    {
      "name-element":"meta",
      "data-attribute":[
        {
          "name-attribute":"name",
          "value-attribute":"google-site-verification"
        },
        {
          "name-attribute":"content",
          "value-attribute":"tdwDvJl9tI5DAnMhFtBr7FQanvM9xrQE3nshBAAaT3Y"
        }
      ],
      "data-innerHTML":"",
      "position":"start"
    },
    {
      "name-element":"meta",
      "data-attribute":[
        {
          "name-attribute":"name",
          "value-attribute":"description"
        },
        {
          "name-attribute":"content",
          "value-attribute":"${descriptionPost}"
        }
      ],
      "data-innerHTML":"",
      "position":"start"
    },
    {
      "name-element":"meta",
      "data-attribute":[
        {
          "name-attribute":"charset",
          "value-attribute":"UTF-8"
        }
      ],
      "data-innerHTML":"",
      "position":"start"
    },
    {
      "name-element":"meta",
      "data-attribute":[
        {
