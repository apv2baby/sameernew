        }
      ],
      "data-innerHTML":"",
      "position":"start"
    },
    {
      "name-element":"meta",
      "data-attribute":[
        {
          "name-attribute":"property",
          "value-attribute":"og:title"
        },
        {
          "name-attribute":"content",
          "value-attribute":"${titlePost}"
        }
      ],
      "data-innerHTML":"",
      "position":"start"
    },
    {
      "name-element":"meta",
      "data-attribute":[
        {
          "name-attribute":"property",
          "value-attribute":"og:description"
        },
        {
          "name-attribute":"content",
          "value-attribute":"${descriptionPost}"
        }
      ],
      "data-innerHTML":"",
      "position":"start"
    },
    {
      "name-element":"meta",
      "data-attribute":[
        {
          "name-attribute":"property",
          "value-attribute":"og:url"
        },
        {
          "name-attribute":"content",
          "value-attribute":"${urlPost}"
